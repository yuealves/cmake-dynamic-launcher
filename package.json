{
  "name": "cmake-dynamic-launcher",
  "displayName": "cmake-dynamic-launcher",
  "description": "automatically change cmake lanuch target",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
            {
                "command": "cmake-dynamic-launcher.setAndRunCurrentFileAsTarget",
                "title": "Set and Run Current C/C++ File as CMake Target"
            }
        ],
        "keybindings": [
            {
                "command": "cmake-dynamic-launcher.setAndRunCurrentFileAsTarget",
                "key": "cmd+r",
                "mac": "cmd+r",
                "win": "alt+r",
                "linux": "alt+r",
                "when": "editorLangId == cpp || editorLangId == c"
            }
        ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
